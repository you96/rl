{% extends "RadiolinecoWebappBundle::layout.ajax.twig" %}

{% block main_container %}
	{% set active_moods = "" %}
	<ul class="breadcrumb">
		  <li><a href="javascript:;" onclick="goUrl('{{ path('radiolineco_webapp_index') }}');$('#li1').removeClass('active'); $('#li2').removeClass('active'); $('#li3').removeClass('active'); $('#li4').removeClass('active');$('#li5').removeClass('active');$('#li1').addClass('active'); return false;" class="ajaxy ajaxy-page" >{{ 'Home'|trans }} </a><span class="divider">&nbsp;&nbsp;&nbsp;>&nbsp;&nbsp;</span></li>
		  {% if moods[0] is defined and moods[0].type == 'tag' %}
		  		{% set active_moods = 'Moods' %}
		  		<li class="active"><h1>{{ 'Listen to'|trans }} {{ 'Radiosh1'|trans }} {{ 'Moods'|trans }} {{ 'radios live'|trans }}</h1></li>
		  {% elseif moodStations['radios'] is defined %}
		  		{% set active_moods = moodStations['tag_info'].body.content.name %}
			  	<li><a href="javascript:;" onclick="goUrl('{{ path('radiolineco_webapp_moods') }}'); return false;" class="ajaxy ajaxy-page nogoUrl" >{{ "Moods"|trans }}</a><span class="divider">&nbsp;&nbsp;&nbsp;>&nbsp;&nbsp;</span></li>
		        <li class="active"><h1>{{ 'Listen to'|trans }} {{ 'Radiosh1'|trans }} {{ active_moods|trans }} {{ 'radios live'|trans }}</h1></li>
		  {% endif %}	  		 
	</ul>
	<!--- begin content-->
	<div class="row">
		<div class="main span8">	
			{% if moods[0] is defined and moods[0].type == 'tag' %}
				{% include "RadiolinecoWebappBundle:Generic:moods_list_bloc.html.twig" %}	
			{% elseif moodStations['radios'] is defined %}
				{#% set active_moods = moodStations['tag_info'].body.content.name %#}
				{% include "RadiolinecoWebappBundle:Generic:moods_stations_bloc.html.twig" %}
			{% endif %}

		
	<!--// end section-->	
					
					
{% endblock %}